                "foregroundImage": "./assets/images/clock.png",
                "backgroundColor": "#476e4e"
            },
            "package": "com.lecha.test"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
