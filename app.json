{
    "expo": {
        "name": "Working Time",
        "slug": "test",
        "version": "1.3.0",
        "orientation": "portrait",
        "icon": "./assets/images/clock.png",
        "scheme": "myapp",
        "userInterfaceStyle": "dark",
        "splash": {
            "image": "./assets/images/hello.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "backgroundColor": "#000000",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/clock.png",
                "backgroundColor": "#476e4e"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        }
    }
}
